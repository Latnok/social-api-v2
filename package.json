{
  "name": "social-api",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "start": "node --enable-source-maps dist/main.js",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write .",
    "prepare": "husky install",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --detectOpenHandles --watch",
    "test:ci": "jest --runInBand --detectOpenHandles --forceExit",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "typeorm-ts-node-commonjs migration:create src/db/migrations/Auto",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate src/db/migrations/Auto -d src/db/data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/db/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/db/data-source.ts",
    "migration:run:test": "NODE_ENV=test typeorm-ts-node-commonjs migration:run -d src/db/data-source.ts"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "argon2": "^0.44.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.9.2",
    "pino-http": "^10.5.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.26"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.1.3",
    "lint-staged": "^16.1.6",
    "mysql2": "^3.14.4",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
